open rust_operators

inl render () =
    leptos.log $'"dark_mode_toggle.render ()"'

    inl global_state : state.global_state = leptos.use_context ()

    leptos.toggle
        fun _ =>
            leptos.rw_signal_get_untracked global_state.core_state .dark_mode |> leptos.signal_update not
        fun () =>
            leptos.rw_signal_get_untracked global_state.core_state .dark_mode |> leptos.rw_signal_get
