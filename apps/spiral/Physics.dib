### try_list_index_of

#!spiral

inl try_list_index_of forall t n {number}. (item : t) (list : list t) : option n =
    inl rec loop (i : n) = function
        | [] => None
        | x :: xs =>
            if x = item
            then Some i
            else loop (i + 1) xs
    loop 0 list

inl list_index_of item =
    try_list_index_of item >> optionm.value

inl try_list_index_of_ forall t n {number}. (item : t) (list : list t) : option n =
    let rec loop (i : n) = function
        | [] => None
        | x :: xs =>
            if x = item
            then Some i
            else loop (i + 1) xs
    loop 0 list

inl list_index_of_ item =
    try_list_index_of_ item >> optionm.value

inl try_list_index_of__ forall t n {number}. (item : t) (list : list t) : option n =
    inl i = mut 0
    inl list = mut list
    inl result = mut None
    let rec loop () =
        match *list with
        | [] => result <- None
        | x :: xs =>
            if x = item
            then result <- Some *i
            else
                i <- *i + 1
                list <- xs
                loop ()
    loop ()
    *result

inl list_index_of__ item =
    try_list_index_of__ item >> optionm.value

