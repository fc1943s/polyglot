## take_while__

#!spiral

inl take_while__ cond sts =
    inl result = mut []
    inl i = mut 0i32
    let rec loop () =
        inl st = sts *i
        if cond st *i then
            result <- st :: *result
            i <- *i + 1
            loop ()
        else *result |> listm.rev
    loop ()

