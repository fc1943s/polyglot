#!fsharp

let escapeCell (input : string) =
    input.Split [| '\n' |]
    |> Array.map (fun line ->
        if line.StartsWith "\\#!"
        then System.Text.RegularExpressions.Regex.Replace (line, "^\\\\#!", "#!")
        else line)
    |> String.concat "\n"

